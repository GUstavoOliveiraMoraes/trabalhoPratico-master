        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Clientes" />
        <activity android:name=".ClienteCad" />
        <activity android:name=".CadCliente" />
        <activity android:name=".Funcionarios" />
        <activity android:name=".CadFuncionario" />
        <activity android:name=".Automoveis" />
        <activity android:name=".CadAutomoveis" />
        <activity android:name=".Locacao" />
